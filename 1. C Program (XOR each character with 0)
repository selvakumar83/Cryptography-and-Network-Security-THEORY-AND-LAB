#include <stdio.h>

int main() {
    char *str = "Hello world";
    char ch;
    int i = 0;

    printf("Original String: %s\n", str);
    printf("After XOR with 0: ");

    while ((ch = str[i]) != '\0') {
        printf("%c", ch ^ 0);  // XOR with 0
        i++;
    }
    printf("\n");

    return 0;
}
